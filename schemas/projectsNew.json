{
    "title": "New Project",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "project_name": {
            "description": "project name",
            "type": "string",
            "minLength": 1,
            "maxLength": 50
        },
        "project_url": {
            "description": "project URL",
            "type": "string",
            "minLength": 1,
            "maxLength": 50
        },
        "github_url1": {
            "description": "github url 1",
            "type": "string",
            "minLength": 1,
            "maxLength": 30,
            "format": "email"
        },
        "github_url2": {
            "description": "github url 2",
            "type": "string",
            "minLength": 1,
            "maxLength": 50
        },
        "project_image_url": {
            "description": "project image URL",
            "type": "string",
            "minLength": 1,
            "maxLength": 50
        },
        "project_descriptions": {
            "description": "project description",
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "project_name",
        "project_url",
        "github_url1",
        "github_url2",
        "project_image_url",
        "project_descriptions"
    ]
}